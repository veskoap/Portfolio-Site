{
  "extends": "./tsconfig.settings",
  "include": [
    ".next/types/**/*.ts",
    "./next-env.d.ts",
    "./src",
    "./sanity-env.d.ts",
    "./sanity.cli.ts",
    "./sanity.config.ts"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "plugins": [{"name": "next"}],
    "paths": {"sanity.config": ["./sanity.config.ts"]}
  }
}
